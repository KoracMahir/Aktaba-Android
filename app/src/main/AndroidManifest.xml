<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mezet.actaandroid">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activities.RegistarRezultatiActivity"></activity>
        <activity android:name=".activities.TestirajBesplatnoActivity" />
        <activity android:name=".activities.KonvertorValutaActivity" />
        <activity android:name=".activities.KreditniKalkulatroActivity" />
        <activity
            android:name=".activities.KursnaListaActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activities.BerzaActivity" />
        <activity android:name=".activities.JedinstvenaVijestActivity" />
        <activity android:name=".activities.pretraga.RegistarCompanyActivity" />
        <activity android:name=".activities.pretraga.PretragaRegistarActivity" />
        <activity android:name=".activities.pretraga.PretragaPromoActivity" />
        <activity android:name=".activities.pretraga.PretragaVijestiActivity" />
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.ProfileActivity" />
        <activity android:name=".activities.TenderiActivity" />
        <activity android:name=".activities.RegistarActivity" />
        <activity android:name=".activities.PromoActivity" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>