<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView241"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/open_sans_bold"
        android:text="BH TELECOM D.D. SARAJEVO"
        android:textColor="@color/akta_cyan"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView241"
        app:srcCompat="@drawable/ic_favorit" />

    <TextView
        android:id="@+id/textView243"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:fontFamily="@font/open_sans"
        android:text="Franca Lehara 7"
        android:textColor="@color/text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView241" />

    <TextView
        android:id="@+id/textView244"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:fontFamily="@font/open_sans"
        android:text="Sarajevo"
        android:textColor="@color/text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView243" />

    <Button
        android:id="@+id/button39"
        android:layout_width="0dp"
        android:layout_height="40sp"
        android:layout_marginTop="20dp"
        android:background="@drawable/textinput_style_cyan"
        android:fontFamily="@font/open_sans_bold"
        android:text="POGLEDAJ BRZI PROFIL"
        android:textColor="@color/akta_cyan"
        android:textSize="@dimen/sesnaest"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView244" />

    <Button
        android:id="@+id/button41"
        android:layout_width="0dp"
        android:layout_height="40sp"
        android:layout_marginTop="20dp"
        android:background="@color/akta_cyan"
        android:fontFamily="@font/open_sans_bold"
        android:text="POGLEDAJ FULL PROFIL"
        android:textColor="@color/white"
        android:textSize="@dimen/sesnaest"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/button39" />

    <View
        android:id="@+id/view27"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:background="@color/border_grey"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/button41" />
</androidx.constraintlayout.widget.ConstraintLayout>