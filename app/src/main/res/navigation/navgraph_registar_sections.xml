        <action
            android:id="@+id/action_vlasnickaStrukturaFragment_to_robneMarkeFragment"
            app:destination="@id/robneMarkeFragment" />
        <action
            android:id="@+id/action_vlasnickaStrukturaFragment_to_robneUslugeFragment"
            app:destination="@id/robneUslugeFragment" />
        <action
            android:id="@+id/action_vlasnickaStrukturaFragment_to_stanjeRacunaFragment"
            app:destination="@id/stanjeRacunaFragment" />
    </fragment>
    <fragment
        android:id="@+id/kapitalFirmeFragment"
        android:name="com.mezet.actaandroid.fragments.main.mojRegistar.sections.KapitalFirmeFragment"
        android:label="fragment_kapital_firme"
        tools:layout="@layout/fragment_kapital_firme" />
    <fragment
        android:id="@+id/finansijeRegistarFragment"
        android:name="com.mezet.actaandroid.fragments.main.mojRegistar.sections.FinansijeRegistarFragment"
