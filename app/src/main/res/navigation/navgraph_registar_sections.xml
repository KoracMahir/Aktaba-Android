        tools:layout="@layout/fragment_izvjestaj_historije_racuna" />
    <fragment
        android:id="@+id/objavljeniTenderiFragment"
        android:name="com.mezet.actaandroid.fragments.main.mojRegistar.sections.ObjavljeniTenderiFragment"
        android:label="fragment_objavljeni_tenderi"
        tools:layout="@layout/fragment_objavljeni_tenderi" />
    <fragment
        android:id="@+id/opisDjelatnostiFragment"
        android:name="com.mezet.actaandroid.fragments.main.mojRegistar.sections.OpisDjelatnostiFragment"
