<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navgraph_registar_sections"
    app:startDestination="@id/vlasnickaStrukturaFragment">

    <fragment
        android:id="@+id/vlasnickaStrukturaFragment"
        android:name="com.mezet.actaandroid.fragments.main.mojRegistar.sections.VlasnickaStrukturaFragment"
        android:label="fragment_vlasnicka_struktura"
        tools:layout="@layout/fragment_vlasnicka_struktura" >
        <action
            android:id="@+id/action_vlasnickaStrukturaFragment_to_kapitalFirmeFragment"
            app:destination="@id/kapitalFirmeFragment" />
        <action
            android:id="@+id/action_vlasnickaStrukturaFragment_to_upravaRegistarFragment"
            app:destination="@id/upravaRegistarFragment" />
        <action
            android:id="@+id/action_vlasnickaStrukturaFragment_to_finansijeRegistarFragment"
