        tools:layout="@layout/fragment_prijavi_gresku" />
    <fragment
        android:id="@+id/robneMarkeFragment"
        android:name="com.mezet.actaandroid.fragments.main.mojRegistar.sections.RobneMarkeFragment"
        android:label="fragment_robne_marke"
        tools:layout="@layout/fragment_robne_marke" />
    <fragment
        android:id="@+id/robneUslugeFragment"
        android:name="com.mezet.actaandroid.fragments.main.mojRegistar.sections.RobneUslugeFragment"
        android:label="fragment_robne_usluge"
        tools:layout="@layout/fragment_robne_usluge" />
    <fragment
        android:id="@+id/upravaRegistarFragment"
        android:name="com.mezet.actaandroid.fragments.main.mojRegistar.sections.UpravaRegistarFragment"
        android:label="fragment_uprava_registar"
        tools:layout="@layout/fragment_uprava_registar" />
    <fragment
        android:id="@+id/stanjeRacunaFragment"
        android:name="com.mezet.actaandroid.fragments.main.mojRegistar.sections.StanjeRacunaFragment"
        android:label="fragment_stanje_racuna"
        tools:layout="@layout/fragment_stanje_racuna" />
</navigation>