        tools:layout="@layout/fragment_opis_djelatnosti" />
    <fragment
        android:id="@+id/prijaviGreskuFragment"
        android:name="com.mezet.actaandroid.fragments.main.mojRegistar.sections.PrijaviGreskuFragment"
